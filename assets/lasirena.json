      "small": "assets/config/logo_light.png",
      "splashScreen": "assets/config/splash.png"
    },
    "icon": "assets/app/app_icon.png",
    "name": "Lasirena",
    "version": "1.0.0",
    "packageName": "com.lasirena.app",
    "theme": {
      "primaryColor": "#0089A7",
      "secondaryColor": "#34C759",
      "backgroundColor": "#FFFFFF",
      "textColor": "#21232F",
      "darkMode": false,
      "arabicFont": "Tajawal",
      "englishFont": "Montserrat"
    },
    "fieldVariation": "OutlinedTopAlignedLabel",
    "homeAppBar": {
      "drawer": "assets/svg/drawer.svg",
