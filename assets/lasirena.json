        {
          "id": "drawer14",
          "title": "drawer.logout",
          "icon": "assets/svg/logout.svg",
          "action": "logout"
        }
      ]
    },
    "appBar": {
      "logo": "assets/images/app_bar_logo.png",
      "title": "الرئيسية",
      "showBackButton": false,
      "actions": [
        {
          "id": "notification",
          "icon": "assets/svg/notification.svg",
          "screen": "notifications"
        },
        {
          "id": "search",
          "icon": "assets/svg/search.svg",
          "action": "search"
        }
      ]
    }
